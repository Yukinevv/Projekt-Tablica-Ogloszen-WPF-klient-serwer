<Window x:Class="Serwer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Serwer"
        mc:Ignorable="d"
        Title="Serwer" Height="450" Width="800" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="325*"/>
            <RowDefinition Height="109*"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="ListBoxPolaczeniKlienci" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="132,24,132,10"/>
        <Button x:Name="UruchomSerwerButton" Content="Uruchom serwer" HorizontalAlignment="Center" VerticalAlignment="Top" Width="120" Height="42" Click="UruchomSerwerButton_Click" Grid.Row="1" Margin="0,13,0,0"/>
    </Grid>
</Window>
