        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Serwer"
        mc:Ignorable="d"
        Title="Serwer" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="ListBox1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="132,10,132,172"/>
        <Button x:Name="SerwerONButton" Content="Serwer ON" HorizontalAlignment="Center" Margin="0,276,0,0" VerticalAlignment="Top" Width="120" Height="42" Click="SerwerONButton_Click"/>
    </Grid>
</Window>
